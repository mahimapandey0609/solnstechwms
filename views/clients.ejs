
<%- include('partials/header') %>
<%- include('partials/nav') %>
<div class="job">
    <div class="job-main" >
        <div >
           
            
        </div> 
        <div class="nav-menu">
            <div class="menu">
               
                <a class="btn btn-light btn-sm mr-2" href="/addclients" role="button" style="background-color: #077b8a; color: white;"> <i class="fa fa-plus pr-1" aria-hidden="true"></i> Add Clients </a>
            </div>
        </div>

   </div>
</div>

<div class="row">
    <div class="col-sm-2">
     <%- include('partials/sidebar3') %>
    
    </div>
    <div class="col-sm-8">
    
    
    <div class="table-wrapper-scroll-y my-custom-scrollbar mt-5">
      <input type="text" id="myInput" onkeyup="myFunction()" placeholder="Search for names..">
        <table class="table table-bordered border-2 table-striped mb-0" id="myTable">
          <thead>
            <tr>
                <th scope="col"></th>
                <th scope="col"></th>
                
              <th scope="col">Client Name</th>
              <th scope="col">Currency</th>
            
              <th scope="col">Billing Method</th>
              <th scope="col">Email Id</th>
              <th scope="col">First Name</th>
              <th scope="col">Last Name</th>
              <th scope="col">Phone</th>
              <th scope="col">Mobile</th>
              <th scope="col">FAX</th>
              <th scope="col">Street Address</th>
              <th scope="col">City</th>
              <th scope="col">State/ province</th>
              <th scope="col">ZIP/PIN</th>
              <th scope="col">Country</th>
              <th scope="col">Industry</th>
              <th scope="col">Company Size</th>
              <th scope="col">Description</th>
            </tr>
          </thead>
          <tbody>
           
            <% clients.forEach(function(e){ %>
    
             <tr>
             
              <td scope="row"> 
                <form action="" method="POST">
                    <input type="checkbox" name="checkbox" value ="<%= %>" onclick="this.form.submit()">
                </form>    
            </td>
            <td>  <a href="" class="btn btn-sm btn-dark">Edit</a></td>
              
              <td scope="row"><%= e.clientName  %> </td>
              <td id="data"><%= e.currency  %></td>
              <td><%= e.bilingmethod %> </td>
              <td><%= e.clientId %></td>
             <td scope="row"><%= e.clientfName %> </td>
              <td id="data"><%=  e.clientlName %></td>
              <td><%= e.clientPhone %></td>
             <td scope="row"><%= e.clientfax %> </td>
              <td id="data"><%= e.clientnumber  %></td>
              <td><%= e.streetaddress %></td>
            <td scope="row"><%= e.clientcity %> </td>
              <td id="data"><%= e.clientState  %></td>
              <td><%= e.clientzip %></td>
              <td scope="row"><%= e.clientcountry %> </td>
              <td id="data"><%= e.clientIndustry  %></td>
              <td scope="row"><%= e.clientcompanySize %> </td>
              <td id="data"><%= e.Description  %></td>
               </tr>
            <% }) %>
          </tbody>
        </table>
      
      </div>
    </div>
    
    </div>
    <%- include('partials/footer') %>
    